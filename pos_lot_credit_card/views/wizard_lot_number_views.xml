<?xml version="1.0"?>

<odoo>

    <record id="view_wizard_session_lot_form" model="ir.ui.view">
        <field name="model">wizard.lot.no</field>
        <field name="arch" type="xml">
            <form string="Session Lot">
                <group>
                    <field name="pos_conf_id" invisible="1"/>
                    <field name="session_id" invisible="1"/>
                    <field name="lot_no"/>
                </group>
                <footer>
                    <button name="confirm_start" type="object" string="Confirm" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
